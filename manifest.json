{
  "name": "All in One",
  "description": "Less Worry",
  "version": "1.0",
  "manifest_version": 3,
    "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage","tabs","activeTab", "scripting"],
   "icons": {
    "16": "image/logo_16x16.png",
    "32": "image/logo_32x32.png",
    "48": "image/logo_48x48.png",
    "128": "image/logo_128x128.png"
  },
   "action": {
    "default_popup": "Popup/popup.html",
      "default_title": "All in One (Less Worry)",
     "default_icon": {
      "16": "image/logo_16x16.png",
      "32": "image/logo_32x32.png",
      "48": "image/logo_48x48.png",
    "128": "image/logo_128x128.png"

    }
  },
  "chrome_url_overrides" : {
    "newtab": "./NewTab/newTab.html"
  },
"options_page": "options.html"
}